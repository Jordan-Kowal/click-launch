{
  "name": "click-launch",
  "version": "2.0.0",
  "type": "module",
  "license": "MIT",
  "author": "Jordan Kowal",
  "homepage": "https://github.com/Jordan-Kowal/click-launch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jordan-Kowal/click-launch.git"
  },
  "bugs": {
    "url": "https://github.com/Jordan-Kowal/click-launch/issues"
  },
  "description": "Desktop app for managing your local dev stack - configure once, launch everything with a click.",
  "keywords": [
    "es6",
    "typescript",
    "ts",
    "solidjs",
    "gui",
    "wails",
    "launcher",
    "process manager",
    "click launch"
  ],
  "scripts": {
    "clean": "rimraf dist bin",
    "reinstall": "rimraf pnpm-lock.yaml node_modules/ && pnpm cache delete && pnpm install",
    "start": "wails3 dev -config ./build/config.yml",
    "build": "wails3 build -config ./build/config.yml",
    "biome:check": "biome check ./src",
    "biome:check:fix": "biome check ./src --write --unsafe",
    "tsc": "tsc",
    "go:test": "go test ./backend/...",
    "go:lint": "golangci-lint run",
    "version:bump": "node scripts/bump-version.mjs"
  },
  "dependencies": {
    "@tanstack/solid-virtual": "3.13.18",
    "@wailsio/runtime": "3.0.0-alpha.79",
    "js-yaml": "4.1.1",
    "uplot": "1.6.32"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.2",
    "@solidjs/router": "0.15.4",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@types/js-yaml": "4.0.9",
    "@types/node": "25.2.3",
    "daisyui": "5.5.18",
    "lucide-solid": "0.574.0",
    "postcss": "8.5.6",
    "rimraf": "6.1.3",
    "solid-devtools": "0.34.5",
    "solid-js": "1.9.11",
    "solid-toast": "0.5.0",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-solid": "2.11.10"
  }
}
